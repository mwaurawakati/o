{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "remote": {
    "urls": ["https://*.*"]
  },
  "permissions": [
    "core:default",
    "opener:default",
    "os:default",
    {
      "identifier": "http:default",
      "allow": [{ "url": "https://*co.il" }],
      "deny": []
    }
  ]
}
